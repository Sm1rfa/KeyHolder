<Window x:Class="KeyHolder.Windows.FilePasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeyHolder.Windows"
        mc:Ignorable="d"
        Title="FilePasswordWindow" Height="100" Width="800"
        WindowStartupLocation="CenterScreen">
    
    
    
    <Grid>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="PassBox" HorizontalAlignment="Left" Height="23" Margin="77,10,0,0" VerticalAlignment="Top" Width="518"/>
        <Button Content="OK" Click="OkClick" HorizontalAlignment="Left" Margin="611,10,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
